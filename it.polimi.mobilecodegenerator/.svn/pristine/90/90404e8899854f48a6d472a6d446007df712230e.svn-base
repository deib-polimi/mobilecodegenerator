<?xml version="1.0" encoding="ASCII"?>
<metamodel:Application
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:metamodel="http://metamodel/1.0"
    xsi:schemaLocation="http://metamodel/1.0 ../metamodel/metamodel.ecore"
    name="Franciacorta"
    companyIdentifier="it.polimi">
  <viewControllers
      name="Home"
      launcher="true">
    <imageViews
        id="homeImageView"
        sourceType="localFile"
        fileUri="franciacorta_bg.png"/>
    <asyncTasks
        name="newsDownloadingTask"/>
    <asyncTasks
        name="businessCategoriesDownloadingTask"/>
    <asyncTasks
        name="businessesDownloadingTask"/>
    <progressDialogs
        id="homeProgressDialog"
        message="Aggiornamenti dati in corso..."/>
  </viewControllers>
  <viewControllers
      name="NewsList">
    <listViews
        id="newsListView">
      <clickListener>
        <navigation
            destination="NewsDetail"/>
      </clickListener>
    </listViews>
  </viewControllers>
  <viewControllers
      name="NewsDetail">
    <webViews
        id="newsDetailWebView"/>
  </viewControllers>
  <viewControllers
      name="RootItinerariesList">
    <listViews
        id="rootItinerariesListView">
      <clickListener>
        <navigation
            destination="ItinerariesList"/>
      </clickListener>
    </listViews>
  </viewControllers>
  <viewControllers
      name="ItinerariesList">
    <listViews
        id="itinerariesListView">
      <clickListener>
        <navigation
            destination="ItineraryDetail"/>
      </clickListener>
    </listViews>
  </viewControllers>
  <viewControllers
      name="ItineraryDetail">
    <webViews
        id="itineraryDetailWebView"/>
  </viewControllers>
  <viewControllers
      name="BusinessCategoriesList">
    <listViews
        id="businessCategoriesListView">
      <clickListener>
        <navigation
            destination="BusinessesList"/>
      </clickListener>
    </listViews>
  </viewControllers>
  <viewControllers
      name="BusinessesList">
    <listViews
        id="businessListView"/>
  </viewControllers>
  <viewControllers
      name="BusinessDetail">
    <webViews
        id="businessDetailView"/>
  </viewControllers>
  <viewControllers
      name="Video">
    <videoView
        id="VideoView"
        sourceType="remoteFile"
        fileUri="http://5630b7e7d9067365084d-c0c9ba06eb3edc1266b332ab9a022e72.r98.cf3.rackcdn.com/franciacorta_app_110422_videoistituzionaleITA_1.mp4"/>
  </viewControllers>
  <viewControllers
      name="LaFranciacorta">
    <buttons
        id="ilFranciacortaButton"
        title="Il Franciacorta">
      <clickListener>
        <navigation
            destination="IlFranciacorta"/>
      </clickListener>
    </buttons>
    <textViews
        id="laFranciacortaTextView"
        content="La Franciacorta"/>
    <webViews
        id="laFranciacortaWebView"
        HTMLFileName="la_franciacorta_it.html"/>
  </viewControllers>
  <viewControllers
      name="IlFranciacorta">
    <buttons
        id="laFranciacortaButton"
        title="La Franciacorta">
      <clickListener>
        <navigation
            destination="LaFranciacorta"/>
      </clickListener>
    </buttons>
    <textViews
        id="ilFranciacortaTextView"
        content="Il Franciacorta"/>
    <webViews
        id="ilFranciacortaWebView"
        HTMLFileName="il_franciacorta_it.html"/>
  </viewControllers>
  <menu>
    <menuItems
        id="item1"
        title="Home">
      <navigation
          destination="Home"/>
    </menuItems>
    <menuItems
        id="newsItem"
        title="News">
      <navigation
          destination="NewsList"/>
    </menuItems>
    <menuItems
        id="videoItem"
        title="Video">
      <navigation
          destination="Video"/>
    </menuItems>
    <menuItems
        id="franciacortaItem"
        title="Franciacorta">
      <navigation
          destination="LaFranciacorta"/>
    </menuItems>
  </menu>
</metamodel:Application>
