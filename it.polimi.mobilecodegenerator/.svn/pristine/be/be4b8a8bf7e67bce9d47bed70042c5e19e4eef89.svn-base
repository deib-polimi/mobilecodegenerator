<?xml version="1.0" encoding="ASCII"?>
<metamodel:Application
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:metamodel="http://metamodel/1.0"
    xsi:schemaLocation="http://metamodel/1.0 ../metamodel/metamodel.ecore"
    name="SendData"
    companyIdentifier="net.polimi"
    companyName="Politecnico di Milano">
  <viewControllers
      name="SendText"
      launcher="true">
    <buttons
        title="Ok"
        id="OkButton">
      <clickListener>
        <navigation
            destination="ReadText"/>
      </clickListener>
    </buttons>
    <buttons
        title="Cancel"
        id="cancelButton">
      <clickListener/>
    </buttons>
    <asyncTasks
        name="downloadImages"/>
    <progressDialogs
        title="titolo"
        message="messaggio"
        id="progressDialogId"/>
  </viewControllers>
  <viewControllers
      name="ReadText">
    <buttons
        title="Empty"
        id="button3"/>
    <listViews
        id="listView2">
      <listViewCells
          title="Uno"/>
      <listViewCells
          title="Due"/>
      <listViewCells
          title="Tre"/>
      <clickListener/>
    </listViews>
    <gridViews
        id="gridView3">
      <gridViewCells
          title="Lucio"/>
    </gridViews>
  </viewControllers>
</metamodel:Application>
