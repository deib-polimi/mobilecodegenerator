<?xml version="1.0" encoding="ASCII"?>
<metamodel:Application
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:metamodel="http://metamodel/1.0"
    xsi:schemaLocation="http://metamodel/1.0 ../metamodel/metamodel.ecore"
    name="SendData"
    companyIdentifier="net.polimi"
    companyName="Politecnico di Milano">
  <viewControllers
      name="SendText"
      launcher="true">
    <buttons
        title="Ok"
        id="OkButton">
      <clickListener>
        <navigation
            destination="ReadText"/>
      </clickListener>
    </buttons>
    <buttons
        title="Cancel"
        id="cancelButton">
      <clickListener/>
    </buttons>
    <imageViews
        id="imageView1"
        fileUri="http://www.blitzquotidiano.it/wp/wp/wp-content/uploads/2010/10/homer-simpson.jpg"/>
    <imageViews
        id="imageView3"
        fileUri="http://img.fotocommunity.com/Paesaggi/Mare/Capri-Panorama-1-a19805542.jpg"/>
    <asyncTasks
        name="downloadImages"/>
    <locationManager/>
  </viewControllers>
  <viewControllers
      name="ReadText">
    <buttons
        title="Empty"
        id="button3"/>
    <imageViews
        id="imageView2"
        fileUri="homer.gif"
        localFile="true"/>
    <imageViews
        id="imageView4"
        fileUri="http://img.fotocommunity.com/Paesaggi/Mare/Capri-Panorama-1-a19805542.jpg"/>
  </viewControllers>
</metamodel:Application>
